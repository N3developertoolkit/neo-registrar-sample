            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/client/cli/bin/Debug/net6.0/cli-client.dll",
            "args": [
                "query",
                "unregistered.domain"
            ],
            "cwd": "${workspaceFolder}/client/cli",
            "console": "internalConsole",
