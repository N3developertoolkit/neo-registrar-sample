    "version": "0.2.0",
    "configurations": [
        {
            "name": "register",
            "type": "neo-contract",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/Debug/net5.0/registrar.nef",
            "neo-express": "${workspaceFolder}/default.neo-express",
            "invocation": {
                "operation": "register",
                "args": [
                    "sample.domain",
                    "@bob"
                ]
            },
            "return-types": ["bool"],
        },
        {
            "name": "register (fail)",
            "type": "neo-contract",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/Debug/net5.0/registrar.nef",
            "neo-express": "${workspaceFolder}/default.neo-express",
            "invocation": {
                "operation": "register",
                "args": [
                    "sample.domain",
                    "@alice"
                ]
            },
            "return-types": ["bool"],
            "checkpoint": "./checkpoints/sample-domain-registered.neoxp-checkpoint",
        },
        {
            "name": "delete",
            "type": "neo-contract",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/Debug/net5.0/registrar.nef",
            "neo-express": "${workspaceFolder}/default.neo-express",
            "invocation": {
                "operation": "delete",
                "args": [
                    "sample.domain"
                ]
            },
            "return-types": ["bool"],
            "checkpoint": "./checkpoints/sample-domain-registered.neoxp-checkpoint",
            "signers": ["bob"]
        }
    ]
}